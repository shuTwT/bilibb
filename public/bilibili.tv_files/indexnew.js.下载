var $tab = $('.tabs');
for (var i=0;i<$tab.length;i++)
{
	$tab[i].id = "jq_tabs_"+i;
}
function c_img_src(obj,lv)
{
	if (typeof(lv)=="undefined") lv=0;
	var lvstr = '';
	for (var i=0;i<lv;i++) lvstr+='  ';
	var childlen = obj.children().size();
	if (!childlen) return;
	for (var i=0;i<childlen;i++)
	{
		if (obj.children()[i].tagName == "DIV")
		{
			if ($(obj.children()[i]).hasClass("po2") || $(obj.children()[i]).hasClass("po"))
			{
				var a_child = $(obj.children()[i]).children("a");
				var img_child = a_child.children("img");
				if(!img_child.attr('src'))
				{
					img_child.attr('src',img_child.attr('_src'));
				}
			}else if ($(obj.children()[i]).children().size())
			{
				c_img_src($(obj.children()[i]),lv+1);
			}
		}
	}
}
function c_img(n){
	var $imgs = $("#jq_tabs_"+n);
	c_img_src($($imgs));
}
var $gogo = $('.nav li');
var $h1 = $('h1');
var $mdiv = $('.m_div');

function gotabs(sindex){
	sindex=parseInt(sindex);
	c_img(sindex);
	$tab.hide();
	$gogo.removeClass();
	$($gogo.get(sindex)).addClass('on');

	$h1.removeClass();
	$h1.addClass('unselectTab');
	$($h1.get(((sindex+1)*2)-2)).removeClass();
	$($h1.get(((sindex+1)*2)-2)).addClass('selectTab');
	//selectTab
	//unselectTab

	$($tab.get(sindex)).show();
	$mdiv.hide();
	$($mdiv.get(((sindex+1)*2)-2)).show();
	//$.cookie('set_nav',sindex,{ expires: 7 , path: '/', domain: '.bilibili.tv'});
	ChatSaveSettings("set_nav",sindex)
}
var mouInterval = null;
var gomouseOn = 0;
var MyInterval = null;
function Ontabs(){
	
	$(function(){ c_img(0); });
	
	if($.browser.msie || $.browser.mozilla || $.browser.opera || $.browser.safari)
	{
		$gogo.mouseover(function(){
			var index = $gogo.index($(this));
			gomouseOn = 1;
			$(this).mouseout(function(){clearInterval(MyInterval);gomouseOn = 0;});
			MyInterval = setTimeout(function(){if(gomouseOn == 1){gotabs(index);}},300);
			return false;
		});
	}
	else
	{
		$gogo.click(function(){
		var index = $gogo.index($(this));
		
		gotabs(index);
		return false;
		});
	}
	$tab.hide();
	$('.tabs:first').show();
	
	$h1.mouseover(function(){
		var index2 = $h1.index($(this));
		$mdiv.hide();
		
		$h1.removeClass();
		$h1.addClass('unselectTab');
		$($h1.get(index2)).removeClass();
		$($h1.get(index2)).addClass('selectTab');
		
		$($mdiv.get(index2)).show();
		return false;
	});
	var $po = $('.tabs .po');
	var $po2 = $('.tabs .po2');
	var mousein = 0;
	$po.mouseover(function(){
		$('.ov-box').css({left:$(this).offset().left,top:$(this).offset().top+80});
		$('.ov-box > .ov > .gk').html($(this).attr('gk'));
		$('.ov-box > .ov > .sc').html($(this).attr('sc'));
		$('.ov-box > .ov > .pl').html($(this).attr('pl'));
		$('.ov-box > .ov > .dm').html($(this).attr('dm'));
		$('.ov-box > .ov > .pf').html("+"+$(this).attr('pf'));
		$('.ov-box > .ov > .tg-date').html("投稿日期:"+$(this).attr('tg'));
		$('.ov-box > .ov > .lm').html($(this).attr('lm'));
		$('.ov-box > .ov > .txt').html($(this).attr('txt'));
		mousein = 1;
		mouInterval = setTimeout(function(){
			console.log('mouse in');
			if(mousein == 1){
				$('.ov-box').fadeIn(300);
				clearInterval(mouInterval);
			}
		},500);
	});
	$('.ov-box').mouseover(function(){$(this).hide();});
	$po2.mouseover(function(){
		$('.ov-box').css({left:$(this).offset().left-235,top:$(this).offset().top-5});
		$('.ov-box > .ov > .gk').html($(this).attr('gk'));
		$('.ov-box > .ov > .sc').html($(this).attr('sc'));
		$('.ov-box > .ov > .pl').html($(this).attr('pl'));
		$('.ov-box > .ov > .dm').html($(this).attr('dm'));
		$('.ov-box > .ov > .pf').html("+"+$(this).attr('pf'));
		$('.ov-box > .ov > .tg-date').html("投稿日期:"+$(this).attr('tg'));
		$('.ov-box > .ov > .lm').html($(this).attr('lm'));
		$('.ov-box > .ov > .txt').html($(this).attr('txt'));
		mousein = 1;
		mouInterval = setTimeout(function(){
			if(mousein == 1){
				clearInterval(mouInterval);
				$('.ov-box').fadeIn(300);
			}
		},500);
	});
	mouseOutFunc = function(){
		mousein=0;
		clearInterval(mouInterval);
		var mouOutInterval = setInterval(function(){
			if (mousein) return;
			clearInterval(mouOutInterval);
			console.log('mouse out');
			$('.ov-box').hide();
		},100);
	};
	$po.mouseout(mouseOutFunc);
	$po2.mouseout(mouseOutFunc);
	$('.ov-box').mouseover(function(){mousein = 1;});
	$('.ov-box').mouseout(mouseOutFunc);
	//$po2.mouseout(function(){$('.ov-box').hide(); mousein=0;})
}
Ontabs();
